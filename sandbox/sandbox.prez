import "stringutil.prez"

class Car {
  public String brand;
  public int mpg;

  public Car(String brand, int mpg) {
    this.brand = brand;
    this.mpg = mpg;
  }

  public String getBrand() {
    return this.brand;
  }
}

void main() {
  Car car = Car("Ford", 26);
  print(car.brand);
  print(car.mpg);

  print(stringutil::strcat("Hello, ", "World!"));
}
