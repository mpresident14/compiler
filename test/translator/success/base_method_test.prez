import "file2.prez"

class Derived : file2::Base {
  override String name() {
    return "Derived";
  }
}

class GrandChild : Derived {
  GrandChild() {
    this.n = 1;
  }

  void sayHi() {
    print("Child of ".append(Derived.name()));
    print("Grandchild of ".append(file2::Base.name()));
  }
}


void main() {
  GrandChild g = GrandChild();
  g.sayHi();
}
